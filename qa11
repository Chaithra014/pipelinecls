pipeline
{
 agent none;
  {
    stages
    {
	stage('BUILD')
	 {
	   agent {label 'slave1'}
	   steps
	    {
	      echo "this is build stagr"
	     }
	  }
	stage('TES')
	 {
	   agent {label 'slave2'}
         parallel
         {
	 stage('test1')
	   {
	   steps
	    {
	      echo "testing on test1 browser"
	     }
          stage('test2')
	   {
	   steps
	    {
	      echo "testing on test2 browser"
	     }

	    }
          }
	}
        stage('DEPLOY')
	 {
	   agent {label 'slave3'}
	   steps
	    {
	      echo "this is deploy stage"
	     }
	  }
       }
  }


}